---
import '../styles/main.css'
import Meta from './partials/meta.astro'

interface Props {
    title?: string
    description?: string
}

const { title, description } = Astro.props
---

<!doctype html>
<html lang="en">
    <head>
        <Meta {title} {description} />
    </head>

    <body>
        <slot />
    </body>
</html>
